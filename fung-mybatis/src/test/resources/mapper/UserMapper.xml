<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="fung.mybatis.mapper.UserMapper" >
  <select id="queryList" resultType="fung.mybatis.pojos.User">
    select * from `user`
  </select>

  <select id="save">
    insert into `user`
    (
      `name`,
      `age`,
      `cid`
    )
    values
    (
      #{name},
      #{age},
      #{cid}
    )
  </select>
</mapper>